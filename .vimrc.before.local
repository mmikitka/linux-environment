" spf13 options {

    " Leader key
    let g:spf13_leader=','

    " Disable wrap relative motion for start/end line motions
    let g:spf13_no_wrapRelMotion = 1

    " Specify the desired bundles
    let g:spf13_bundle_groups=['general', 'writing', 'neocomplcache', 'programming', 'php', 'ruby', 'python', 'javascript', 'html', 'misc', 'debugger']

    " Disable views
    let g:spf13_no_views = 1
" }

" Disable Go auto-install
let g:go_disable_autoinstall = 1
